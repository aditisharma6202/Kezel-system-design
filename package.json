{
  "name": "kezel-system-design",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "dev": "pnpm --filter docs run dev",
    "storybook": "pnpm --filter docs run storybook",
    "build-storybook": "pnpm --filter docs run build-storybook",
    "build": "turbo run build",
    "typecheck": "turbo run typecheck",
    "lint": "eslint packages apps/docs/app apps/docs/stories apps/docs/.storybook",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "changeset": "changeset",
    "version": "changeset version",
    "publish": "pnpm build && changeset publish",
    "size": "pnpm --filter kz-design-system run size"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "globals": "^15.14.0",
    "eslint-config-next": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.3.3",
    "turbo": "^2.3.0",
    "typescript": "^5.6.3"
  }
}
